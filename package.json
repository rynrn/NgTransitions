{
  "name": "ng-transitions",
  "version": "3.1.4",
  "homepage": "https://github.com/rynrn/NgTransitions",
  "repository": "https://github.com/rynrn/NgTransitions",
  "scripts": {
    "build": "tsc --sourceMap",
    "lint": "ng lint",
    "verify": "npm run lint && npm run test && npm run build",
    "clean": "rimraf ./ng-transitions",
    "copy:json:unix": "cp package.json ng-transitions/package.json",
    "copy:json:win": "copy .\\package.json .\\ng-transitions\\package.json /Y",
    "deploy:unix": "npm run clean && tsc && npm run copy:json:unix && cd ng-transitions && npm publish",
    "deploy:win": "npm run clean && tsc && npm run copy:json:win && cd ng-transitions && npm publish"
  },
  "dependencies": {},
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.2",
    "@angular/cli": "~7.1.0",
    "@angular/common": "^7.1.0",
    "@angular/compiler": "^7.1.0",
    "@angular/compiler-cli": "^7.1.0",
    "@angular/core": "^7.1.0",
    "@angular/language-service": "^7.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "animate.css": "^3.6.1",
    "codelyzer": "~4.2.1",
    "core-js": "^2.5.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.2",
    "rxjs": "^6.0.0",
    "systemjs-builder": "^0.16.13",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.1.6",
    "zone.js": "^0.8.26"
  }
}