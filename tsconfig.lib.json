{
  "compilerOptions": {
    "declaration": true,
    "module": "es2015",
    "target": "es5",
    "baseUrl": ".",
    "stripInternal": true,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "outDir": "./out-tsc/lib",
    "rootDir": ".",
    "lib": [
      "es2016",
      "dom"
    ],
    "skipLibCheck": true,
    "types": [],
  },
  "angularCompilerOptions": {
    "enableIvy": false,
    "annotateForClosureCompiler": false,
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true
  },
  "files": [
    "./public_api.ts"
  ]
}